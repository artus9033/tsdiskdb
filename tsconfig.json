{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"lib": ["es2016"],
		"allowSyntheticDefaultImports": true,
		"target": "ES5",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": "src",
		"declaration": true,
		"types": ["node"]
	}
}
